<h1>Rent our fluffy cats</h1>

<form class="" action="<%= users_url %>" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">

  <label for="username">Username</label>
  <input id="username" type="text" name="users[username]" value="<%= @user.username %>">

  <label for="password">Password</label>
  <input id="password" type="password" name="users[password]">


  <input type="submit" value="Register">
</form>
